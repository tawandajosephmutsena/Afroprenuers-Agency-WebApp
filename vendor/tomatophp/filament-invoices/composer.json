{
    "name": "tomatophp/filament-invoices",
    "type": "library",
    "description": "Generate and manage your invoices / payments using multi currencies and multi types in FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "invoices",
        "payments",
        "filament",
        "filament-plugin",
        "tomatophp",
        "tomato-php"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\FilamentInvoices\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\FilamentInvoices\\FilamentInvoicesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "tomatophp/console-helpers": "^1.1",
        "tomatophp/filament-locations": "^2.0",
        "tomatophp/filament-types": "^2.0",
        "filament/filament": "^3.0.0"
    }
}
